$(document).ready(function(){
	var i=0;
	$(".test .body").each(function(){
		var grade = 0;
		$(".test .body").eq(i).find(".grade p").each(function(){
			grade += parseInt($(this).text());
		});
		
		if($(".test .body").eq(i).find(".lead"))
		
		$(".test .body").eq(i).find(".grade").text(grade+"Ï†ê");
		i++;
	});
});